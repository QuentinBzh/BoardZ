<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MdlEventSubscriptions" table="mdl_event_subscriptions">
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="url" type="string" column="url" length="255" nullable="false"/>
    <field name="courseid" type="bigint" column="courseid" nullable="false"/>
    <field name="groupid" type="bigint" column="groupid" nullable="false"/>
    <field name="userid" type="bigint" column="userid" nullable="false"/>
    <field name="eventtype" type="string" column="eventtype" length="20" nullable="false"/>
    <field name="pollinterval" type="bigint" column="pollinterval" nullable="false"/>
    <field name="lastupdated" type="bigint" column="lastupdated" nullable="true"/>
    <field name="name" type="string" column="name" length="255" nullable="false"/>
  </entity>
</doctrine-mapping>
